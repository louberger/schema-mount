module example-logical-devices {
  namespace "urn:example:logical-devices";
  prefix exld;

  import ietf-yang-structural-mount {
    prefix yangmnt;
  }

  container logical-devices {
    list logical-device {
      key name;
      leaf name {
        type string;
      }

      yangmnt:mount-point logical-device {
        yangmnt:use-yang-library;
      }
    }
  }
}
